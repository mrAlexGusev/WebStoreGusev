@model IEnumerable<WebStoreGusev.Models.EmployeeViewModel>
@{
    ViewData["Title"] = "Index";
}

<h1>Hello from my first view</h1>

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.LastName</td>
            <td>@item.FirstName</td>
            <td>@item.Patronymic</td>
        </tr>
    }
</table>